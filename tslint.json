{
    "defaultSeverity": "error",
    "extends": ["@vovkasm/tslint-config"],
    "rulesDirectory": ["tslint-plugin-prettier"],
    "rules": {
        "quotemark": { "options": ["single", "jsx-double"] },
        "object-literal-key-quotes": { "options": ["as-needed"] },
        "prettier": true,
        "space-before-function-paren": { "severity": "off" },
        "trailing-comma": { "severity": "off" }
    }
}
